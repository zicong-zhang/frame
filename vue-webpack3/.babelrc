{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": [
          "safari >= 8",
          "last 2 versions"
        ]
      },
      "useBuiltIns": "usage"
    }]
  ],
  "plugins": [
    "transform-runtime",
    "transform-object-rest-spread",
    "syntax-dynamic-import"
  ]
}
